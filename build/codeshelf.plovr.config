{
	id: 'codeshelf',
	paths: [
		'../lib/JQuery', 
		'../lib/SlickGrid', 
		'../lib/Raphael', 
		'../src/js_uncompiled',
		'../lib/GoogleCLosureLibrary/third_party',
		'../lib/GoogleCLosureTemplates'
	],
	"checks": {
		// Unfortunately, the Closure Library violates these in many places.
		// "accessControls": "ERROR",
		// "visibility": "ERROR"
		"checkRegExp": "OFF",
		"checkTypes": "OFF",
		"checkVars": "OFF",
		"deprecated": "OFF",
		"fileoverviewTags": "OFF",
		"invalidCasts": "OFF",
 		"missingProperties": "OFF",
		"nonStandardJsDocs": "OFF",
		"undefinedVars": "OFF"
	},
	inputs: '../src/js_uncompiled/application.js',
	output-file: '../src/js_compiled/codeshelf.js',
//	variable-map-output-file: "../src/js_compiled/variables.map",
//	property-map-output-file: "../src/js_compiled/properties.map",
	externs:  '../src/js_externs/',
	//output-wrapper: '(function(){%output%})();',
	closure-library: '../lib/GoogleClosureLibrary/closure/goog/',
	mode: 'ADVANCED',
	debug: true,
	pretty-print: false,
	disambiguate-properties: true,
	ambiguate-properties: true
//	"id-generators": ["goog.events.getUniqueId"]
}
